my @extra = (
    qq|10.10.10.2:22500   192.168.5.3:80   203.0.113.5:7072    198.51.31.1:80  tcp     1|,
    qq|10.10.10.5:33500   192.168.6.3:80   203.0.113.8:7068    198.51.31.1:443  tcp     10|,
    qq|10.10.10.1:10100   192.168.5.3:80   203.0.113.5:7060    198.51.31.1:80  tcp     10|,
    qq|10.10.20.4:10100   192.168.5.3:80   203.0.113.7:7060    198.51.32.1:80  tcp     10|,
);

foreach my $y (@extra) {
    if (int(rand(2))) {
        my $x = int(rand($#lines));
        $lines[$x] = $y;
    }
}

print join( "\n", @lines ), "\n";
