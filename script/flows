    qq|10.10.10.4:40000   192.168.5.3:80   203.0.113.5:6066    198.51.31.1:80  tcp     105|,
    qq|10.10.10.4:40001   192.168.5.3:80   203.0.113.5:6066    198.51.31.1:80  tcp     105|,
    qq|10.10.10.5:50000   192.168.5.3:80   203.0.113.5:6068    198.51.31.1:80  tcp     10|,
    qq|10.10.10.1:11000   192.168.5.3:80   203.0.113.5:6070    198.51.31.1:80  tcp     16|,
    qq|10.10.10.1:11000   192.168.5.3:80   203.0.113.6:6070    198.51.31.2:80  tcp     6|,
    qq|10.10.10.2:12000   192.168.5.3:80   203.0.113.5:6072    198.51.31.1:80  tcp     1|,
